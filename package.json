    "graphql": "^16.11.0",
    "ioredis": "^5.6.1",
    "mongodb": "^6.16.0",
    "mongoose": "^8.15.0",
    "mysql2": "^3.14.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.24"
